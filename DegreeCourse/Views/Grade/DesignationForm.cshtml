@model Model.ModelGradeM.ModelGrade

<div class="row">
    <div class="col-md-12">
        <form asp-action="DesignationForm" asp-route-id="@Model" autocomplete="off" method="post">
            <input type="hidden" asp-for="GradeId" />

            <div class="row">
                <div class="col-lg-12 mb-3">
                    <label class="form-label" asp-for="EnrollmentId">EnrollmentId</label>
                    <input type="text" asp-for="EnrollmentId" class="form-control" autocomplete="off" />
                    <span asp-validation-for="EnrollmentId" class="text-danger"></span>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 mb-3">
                    <label class="form-label" asp-for="Grade1">Grade1</label>
                    <input type="text" asp-for="Grade1" class="form-control" autocomplete="off" />
                    <span asp-validation-for="Grade1" class="text-danger"></span>
                </div>
            </div>

            <div class="row">
                <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                    <input type="submit" value="Save" class="btn btn-primary" />
                    <a href="/Grade/Designation" class="btn btn-danger">Back</a>
                </div>
            </div>
        </form>
    </div>
</div>